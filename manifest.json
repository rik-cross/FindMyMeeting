{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Find My Meeting",
  "description" : "Automatically switch to an active Google Meet tab",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "128": "128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": {      
      "16": "16.png", 
      "24": "32.png", 
      "32": "128.png" 
    },
    "default_title": "Find my meeting"
  }
}
